    <id>pulse</id>
    <title>pulse chat server</title>
    <version>1.0.0</version>
    <authors>DevFacto</authors>
    <owners>DevFacto</owners>
    <licenseUrl>http://devfacto.com</licenseUrl>
    <projectUrl>http://devfacto.com</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Internal DevFacto Web App</description>
